{
  "name": "instagrapi",
  "version": "1.0.4",
  "description": "Library to obtain information from an Instagram account in a friendly and intuitive way",
  "keywords": [
    "instagram",
    "api",
    "nodejs",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Edixon Alberto",
    "email": "edixonalbertto@gmail.com",
    "url": "https://www.edixonalberto.com"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "node test",
    "lint": "prettier --write ./src/**/*.ts",
    "dev": "nodemon -e ts -w ./src --exec \"ts-node src\"",
    "prepublish": "yarn lint && yarn clean && yarn build && yarn distribute",
    "distribute": "browserify dist/index.js --standalone instagrapi -o dist/bundle.js",
    "build": "tsc",
    "start": "node dist"
  },
  "dependencies": {
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.4",
    "browserify": "^17.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": "^12.18.0",
    "yarn": "^1.22.5"
  },
  "homepage": "https://github.com/EdixonAlberto/instagrapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdixonAlberto/instagrapi.git"
  },
  "bugs": {
    "url": "https://github.com/EdixonAlberto/instagrapi/issues"
  }
}