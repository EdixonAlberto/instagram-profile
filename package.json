{
  "name": "instagrapi",
  "version": "3.2.3",
  "description": "Library to obtain information from an Instagram account in a friendly and intuitive way",
  "keywords": [
    "instagram",
    "api",
    "nodejs",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Edixon Pi√±a",
    "email": "edixonalbertto@gmail.com",
    "url": "https://edixonalberto.com"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "lint": "prettier --write src/**/*.ts demo/**/*.{js,css,html}",
    "dev": "nodemon --config",
    "pre-publish": "yarn lint && yarn clean && yarn build && yarn bundle && yarn minify",
    "bundle": "node tools/createBundle.js",
    "minify": "node tools/minifyBundle.js",
    "build": "tsc",
    "start": "node dist",
    "deploy": "gh-pages -o github -d ./demo",
    "serve": "live-server --port=80 ./demo"
  },
  "nodemonConfig": {
    "ext": ".ts,.env",
    "watch": [
      "./",
      "src/*"
    ],
    "verbose": false,
    "exec": "yarn build && node dist",
    "ignore": [
      "node_modules/",
      "dist/",
      "tests/"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.17",
    "browserify": "^17.0.0",
    "dotenv": "^10.0.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.5.4",
    "uglify-js": "^3.12.1"
  },
  "homepage": "https://github.com/EdixonAlberto/instagrapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdixonAlberto/instagrapi.git"
  },
  "bugs": {
    "url": "https://github.com/EdixonAlberto/instagrapi/issues"
  }
}
